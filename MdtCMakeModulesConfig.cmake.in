@PACKAGE_INIT@

include("${CMAKE_CURRENT_LIST_DIR}/MdtCMakeModulesMdtFindPathInList.cmake")

# Add to CMAKE_MODULE_PATH if not allready
MdtCMakeModules_mdt_find_path_in_list(CMAKE_MODULE_PATH "${PACKAGE_PREFIX_DIR}/@MDT_CMAKE_MODULE_INSTALL_MODULES_DIR@" MDT_CMAKE_MODULE_PATH_INDEX)
if(${MDT_CMAKE_MODULE_PATH_INDEX} LESS 0)
  list(APPEND CMAKE_MODULE_PATH "${PACKAGE_PREFIX_DIR}/@MDT_CMAKE_MODULE_INSTALL_MODULES_DIR@")
endif()

unset(MDT_CMAKE_MODULE_PATH_INDEX)
