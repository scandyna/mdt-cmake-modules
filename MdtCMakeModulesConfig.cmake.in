@PACKAGE_INIT@

set(MDT_CMAKE_MODULE_PATH "${PACKAGE_PREFIX_DIR}/@MDT_CMAKE_MODULE_INSTALL_MODULES_DIR@")

message("PACKAGE_PREFIX_DIR: ${PACKAGE_PREFIX_DIR}")
message("MDT_CMAKE_MODULE_PATH: ${MDT_CMAKE_MODULE_PATH}")

# Add to CMAKE_MODULE_PATH if not allready
list(FIND CMAKE_MODULE_PATH "${MDT_CMAKE_MODULE_PATH}" MDT_CMAKE_MODULE_PATH_INDEX)
if(${MDT_CMAKE_MODULE_PATH_INDEX} LESS 0)
  list(APPEND CMAKE_MODULE_PATH "${MDT_CMAKE_MODULE_PATH}")
endif()
