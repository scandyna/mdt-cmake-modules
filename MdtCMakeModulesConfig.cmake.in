@PACKAGE_INIT@

include("${CMAKE_CURRENT_LIST_DIR}/MdtCMakeModulesMdtFindPathInList.cmake")

# This variable is made available to the user
set(MDT_CMAKE_MODULES_PATH "${PACKAGE_PREFIX_DIR}/@MDT_CMAKE_MODULE_INSTALL_MODULES_DIR@")

# Add to CMAKE_MODULE_PATH if not allready
MdtCMakeModules_mdt_find_path_in_list(CMAKE_MODULE_PATH "${MDT_CMAKE_MODULES_PATH}" MDT_CMAKE_MODULE_PATH_INDEX)
if(${MDT_CMAKE_MODULE_PATH_INDEX} LESS 0)
  list(APPEND CMAKE_MODULE_PATH "${MDT_CMAKE_MODULES_PATH}")
endif()

set(MdtCMakeModules_MdtFindPathInList_IN_FILE "@PACKAGE_modulesInstallDir@")

unset(MDT_CMAKE_MODULE_PATH_INDEX)
